DROP TABLE IF EXISTS `responses_one`;
CREATE TABLE `responses_one` (
  `id` int(11) NOT NULL AUTO_INCREMENT,
  `voter_sunet` varchar(50) DEFAULT NULL,
  `qid` int(11) DEFAULT NULL,
  `nid` int(11) DEFAULT NULL,
  PRIMARY KEY (`id`),
  UNIQUE KEY `voter_qid` (`voter_sunet`,`qid`)
) ENGINE=MyISAM AUTO_INCREMENT=3498 DEFAULT CHARSET=latin1;
DROP TABLE IF EXISTS `responses_two`;
CREATE TABLE `responses_two` (
  `id` int(11) NOT NULL AUTO_INCREMENT,
  `voter_sunet` char(50) DEFAULT NULL,
  `qid` int(11) DEFAULT NULL,
  `nid1` int(11) DEFAULT NULL,
  `nid2` int(11) DEFAULT NULL,
  PRIMARY KEY (`id`),
  UNIQUE KEY `voter_question` (`voter_sunet`,`qid`)
) ENGINE=MyISAM AUTO_INCREMENT=266 DEFAULT CHARSET=latin1;
